      </Label>
      <Label fx:id="" alignment="CENTER" layoutX="883.0" layoutY="670.0" prefHeight="34.0" prefWidth="113.0" text="THAI" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>

      <!-- On Action will trigger an event of the given name inside the controller class-->
      <Button layoutX="17.0" layoutY="110.0" mnemonicParsing="false" onAction="#indianPressed" opacity="0.0" prefHeight="250.0" prefWidth="250.0" text="Button">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <!--Cursor will  change to hand when it hovers over the button-->
      <!-- mnemonic Parsing enables you to press a key after _ character which will trigger the event
      Example if the text was ba_ck then Alt + C would trigger the onAction-->
      <Button fx:id="chineseButton" layoutX="286.0" layoutY="110.0" mnemonicParsing="false" onAction="#chinesePressed"
              opacity="0.0" prefHeight="250.0" prefWidth="250.0" text="Button">
