        <Label layoutX="95.0" layoutY="168.0" prefHeight="38.0" prefWidth="300.0" text="- 1/4 cup white miso paste">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label layoutX="92.0" layoutY="195.0" prefHeight="31.0" prefWidth="363.0" text="- 2 tablespoons sugar">
